@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_die ;
  lemon:entry    :in .

:to_die a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_die ;
  lemon:otherForm      :form_died ;
  lemon:synBehavior    :die_frame ;
  lemon:sense          :die_ontomap .

:form_die a         lemon:Form ;
  lemon:writtenRep     "die"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_died a   lemon:Form ;
  lemon:writtenRep "died"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:past .

:die_frame a                  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :die_subj ;
  lexinfo:prepositionalAdjunct :die_obj .

:die_ontomap a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :die_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/deathPlace> ;
  lemon:subjOfProp  :die_obj ;
  lemon:objOfProp   :die_subj ;
  lemon:condition   :die_condition .


:die_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Place> .

:die_obj lemon:marker :in .

## Prepositions ##

:in a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .