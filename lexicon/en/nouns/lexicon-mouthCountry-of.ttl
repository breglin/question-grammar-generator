@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :mouthCountry_of ;
  lemon:entry    :of .

:mouthCountry_of a          lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :mouthCountry_form ;
  lemon:synBehavior    :mouthCountry_of_nounpp ;
  lemon:sense          :mouthCountry_sense_ontomap .

:mouthCountry_form a    lemon:Form ;
  lemon:writtenRep "mouth country"@en .

:mouthCountry_of_nounpp a           lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:mouthCountry_sense_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping      :mouthCountry_sense_ontomap ;
  lemon:reference        <http://dbpedia.org/ontology/mouthCountry> ;
  lemon:subjOfProp       :arg2 ;
  lemon:objOfProp        :arg1 ;
  lemon:condition        :mouthCountry_condition .

:mouthCountry_condition a   lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/River> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Country> .

:arg2 lemon:marker :of .

## Prepositions ##

:of a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .