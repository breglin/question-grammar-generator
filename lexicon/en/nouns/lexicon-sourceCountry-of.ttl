@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :sourceCountry_of ;
  lemon:entry    :of .

:sourceCountry_of a          lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :sourceCountry_form ;
  lemon:synBehavior    :sourceCountry_of_nounpp ;
  lemon:sense          :sourceCountry_sense_ontomap .

:sourceCountry_form a    lemon:Form ;
  lemon:writtenRep "source country"@en .

:sourceCountry_of_nounpp a           lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:sourceCountry_sense_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping      :sourceCountry_sense_ontomap ;
  lemon:reference        <http://dbpedia.org/ontology/sourceCountry> ;
  lemon:subjOfProp       :arg2 ;
  lemon:objOfProp        :arg1 ;
  lemon:condition        :sourceCountry_condition .

:sourceCountry_condition a   lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Stream> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Country> .

:arg2 lemon:marker :of .

## Prepositions ##

:of a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .
