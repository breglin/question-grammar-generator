@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :rightTributary_of ;
  lemon:entry    :of .

:rightTributary_of a      lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :rightTributary_form ;
  lemon:otherForm      :rightTributaries_form ;
  lemon:synBehavior    :rightTributary_of_nounpp ;
  lemon:sense          :rightTributary_sense_ontomap .

:rightTributary_form a lemon:Form ;
  lemon:writtenRep  "right tributary"@en .

:rightTributaries_form a lemon:Form ;
  lemon:writtenRep   "right tributaries"@en ;
  lexinfo:number     lexinfo:plural .

:rightTributary_of_nounpp a       lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:rightTributary_sense_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping          :rightTributary_sense_ontomap ;
  lemon:reference            <http://dbpedia.org/ontology/rightTributary> ;
  lemon:subjOfProp           :arg2 ;
  lemon:objOfProp            :arg1 ;
  lemon:condition            :rightTributary_condition .

:rightTributary_condition a lemon:condition ;
  lemon:propertyDomain   <http://dbpedia.org/ontology/River> ;
  lemon:propertyRange    <http://dbpedia.org/ontology/River> .

:arg2 lemon:marker :of .

## Prepositions ##

:of a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .