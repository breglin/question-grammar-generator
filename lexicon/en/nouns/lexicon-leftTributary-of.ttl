@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :leftTributary_of ;
  lemon:entry    :of .

:leftTributary_of a      lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :leftTributary_form ;
  lemon:otherForm      :leftTributaries_form ;
  lemon:synBehavior    :leftTributary_of_nounpp ;
  lemon:sense          :leftTributary_sense_ontomap .

:leftTributary_form a lemon:Form ;
  lemon:writtenRep  "left tributary"@en .

:leftTributaries_form a lemon:Form ;
  lemon:writtenRep   "left tributaries"@en ;
  lexinfo:number     lexinfo:plural .

:leftTributary_of_nounpp a       lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:leftTributary_sense_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping          :leftTributary_sense_ontomap ;
  lemon:reference            <http://dbpedia.org/ontology/leftTributary> ;
  lemon:subjOfProp           :arg2 ;
  lemon:objOfProp            :arg1 ;
  lemon:condition            :leftTributary_condition .

:leftTributary_condition a lemon:condition ;
  lemon:propertyDomain   <http://dbpedia.org/ontology/River> ;
  lemon:propertyRange    <http://dbpedia.org/ontology/River> .

:arg2 lemon:marker :of .

## Prepositions ##

:of a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .